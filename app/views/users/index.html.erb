<p id="notice"><%= notice %></p>

<h1>User</h1>

<table>
  <thead>
  <tr>
    <th>name</th>
    <th>email</th>
    <th>address</th>
    <th>about_me</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.name %></td>
      <td><%= user.email %></td>
      <td><%= user.address %></td>
      <td><%= user.about_me %></td>
      <td><%= link_to "詳細", user %></td>
    </tr>
  <% end %>
  </tbody>
</table>

<%= paginate @users %>



