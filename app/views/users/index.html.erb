<p id="notice"><%= notice %></p>

<h1>User</h1>

<table>
  <thead>
  <tr>
    <th>id</th>
    <th>email</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.id %></td>
      <td><%= user.email %></td>
      <td><%= link_to "詳細", user %></td>
    </tr>
  <% end %>
  </tbody>
</table>

<%= paginate @users %>

